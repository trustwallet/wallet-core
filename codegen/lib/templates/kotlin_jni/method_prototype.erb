<%
  method = locals[:method]
  if method.static
    parameters = '[[maybe_unused]] jclass thisClass' + KotlinJniHelper.parameters(method.parameters)
  else
    parameters = '[[maybe_unused]] jobject thisObject' + KotlinJniHelper.parameters(method.parameters.drop(1))
  end
-%>
<%= KotlinJniHelper.type(method.return_type) %> JNICALL <%= KotlinJniHelper.function_name(entity: entity, function: method) %>([[maybe_unused]] JNIEnv *env, <%= parameters %>)<% -%>
