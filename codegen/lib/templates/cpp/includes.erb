<%# Include entity headers -%>
<% require 'set' -%>
<%  includes = Set.new -%>
<%  includes.add({name: entity.name, generated: entity.generated?}) -%>
<%  (entity.static_methods + entity.methods).each do |method| -%>
<%    if method.return_type.is_struct || method.return_type.is_class -%>
<%      includes.add({name: method.return_type.name, generated: method.return_type.is_generated}) -%>
<%    end -%>
<%    method.parameters.each do |param| -%>
<%      if param.type.is_struct || param.type.is_class -%>
<%        includes.add({name: param.type.name, generated: param.type.is_generated}) -%>
<%      end -%>
<%    end -%>
<%  end -%>
<%  includes.each do |include| -%>
<%    if include[:generated] -%>
#include <TrustWalletCore/Generated/TW<%= include[:name] %>.h>
<%    else -%>
#include <TrustWalletCore/TW<%= include[:name] %>.h>
<%    end -%>
<%  end -%>

<%  includes.each do |include| -%>
<%  next if include[:name] == entity.name -%>
#include "./<%= include[:name] %>.h"
<%  end -%>

#include <emscripten/bind.h>

using namespace emscripten;
